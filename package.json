{
  "name": "@rutan/frame-tween",
  "version": "0.5.0",
  "author": "ru_shalm",
  "email": "ru_shalm@hazimu.com",
  "license": "WTFPL",
  "homepage": "https://github.com/rutan/frame-tween",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rutan/frame-tween"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "esm/index.js",
  "files": [
    "src/",
    "lib/",
    "esm/"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "run-s lint test clean build:*",
    "build:cjs": "cross-env NODE_ENV=production tsc -p ./tsconfig.json",
    "build:esm": "cross-env NODE_ENV=production tsc -p ./tsconfig.mjs.json",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier -c .",
    "lint:ts": "tsc --noEmit",
    "test": "jest",
    "format": "prettier -w .",
    "clean": "run-p clean:*",
    "clean:cjs": "rimraf ./cjs",
    "clean:mjs": "rimraf ./mjs",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}

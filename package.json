{
  "name": "@rutan/frame-tween",
  "version": "0.4.0",
  "author": "ru_shalm",
  "email": "ru_shalm@hazimu.com",
  "license": "WTFPL",
  "homepage": "https://github.com/rutan/frame-tween",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rutan/frame-tween"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "esm/index.js",
  "files": [
    "src/",
    "lib/",
    "esm/"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "run-s test clean build:*",
    "build:cjs": "cross-env NODE_ENV=production tsc -p ./tsconfig.json",
    "build:esm": "cross-env NODE_ENV=production tsc -p ./tsconfig.esm.json",
    "clean": "run-p clean:*",
    "clean:lib": "rimraf ./lib",
    "clean:esm": "rimraf ./esm",
    "watch": "run-p watch:*",
    "watch:cjs": "tsc -w -p ./tsconfig.json",
    "watch:esm": "tsc -w -p ./tsconfig.esm.json",
    "test": "jest",
    "format": "pretty-quick",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "cross-env": "^7.0.3",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
